-var id = id;
-var start = start;
-var end = end;
div
  form(method="GET", id=id, class="time-form")
    table
      tr
        td
          label(for="start") Start
        td
          input(type="text", name="start", value=start, style="margin-right: 15px;")
        td
          label(for="end") End
        td
          input(type="text", name="end", value=end)
        td
          input(type="submit", value=" GO! ", class="form-submit", style="width: 70px; height: 30px;")
